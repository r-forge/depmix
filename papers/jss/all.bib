@techreport{Stolcke1994,
	Author = {Andreas Stolcke and Stephen M. Omohundro},
	Date-Added = {2010-03-03 16:04:34 +0100},
	Date-Modified = {2010-03-03 16:04:34 +0100},
	Institution = {International Computer Science Institute, University of California at Berkeley},
	Note = {Available from http://www.icsi.berkeley.edu/techreports/1994.html},
	Number = {TR--94--003},
	Summary = {Authors use HMMs for automata induction from data. They outline a Bayesian approach to estimation of parameters, starting out with a saturated model, i.e.\ a model that has a state for each data point. This latter feature renders the method impractical for large datasets although authors mention online learning as an option. Most priors they chose are non-informative except possibly the structure prior for the number of states in a model which effectively gives a penalty for increasing numbers of states. However, the magnitude of this penalty is not given. In their automata induction example authors use a description length prior, which comes down to counting the number of non-zero parameters (which implicitly incorporates the number of states). Furthermore they use a Dirichlet prior for the parameter values which is biased to produce zeroes and ones in the transition and emission matrices but is generally uninformative. Also they maximize the Maximum Posterior Probability. The method seems very much to be identical to using AIC as selection criterion where the AIC is adjusted to only count the non-zero parameters.},
	Title = {Best-first model merging for hidden Markov model induction},
	Year = 1994}

